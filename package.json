{
  "name": "confetti-cli",
  "version": "0.0.15",
  "description": "Command line interface for Confetti.",
  "scripts": {
    "clean": "rimraf ./bin/*",
    "lint": "./node_modules/.bin/eslint ./src",
    "compile": "npm run clean && npm run lint && babel -d bin/ src/"
  },
  "repository": "andreamangano/confetti-cli",
  "author": "Andrea Mangano <andreaman87@gmail.com> (http://andreamangano.com)",
  "license": "ISC",
  "dependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "cwd": "^0.10.0",
    "empty-dir": "^0.2.1",
    "is-there": "^4.3.3",
    "shelljs": "^0.7.4",
    "yargs": "^5.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-preset-es2015": "^6.16.0",
    "eslint": "^3.10.2",
    "eslint-config-xo-space": "^0.15.0",
