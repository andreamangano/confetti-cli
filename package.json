  "author": "Andrea Mangano <andreaman87@gmail.com> (http://andreamangano.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/andreamangano/confetti-cli"
  },
  "bugs": {
    "url": "https://github.com/andreamangano/confetti-cli/issues"
  },
  "dependencies": {
    "browser-sync": "^2.18.6",
    "chalk": "^1.1.3",
    "confetti-generator": "^0.1.19",
    "confetti-loader": "^0.0.19",
    "del": "^2.2.2",
    "empty-dir": "^0.2.1",
    "is-there": "^4.3.3",
    "shelljs": "^0.7.4",
    "yargs": "^5.0.0"
